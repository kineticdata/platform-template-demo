      "version": 1,
      "parameters": [
        {
          "dependsOnId": "",
          "dependsOnValue": "",
          "description": "JSON Object Representing the Body",
          "id": "Body",
          "label": "Body",
          "menu": "",
          "value": "<%= \nvalues = JSON.parse(@results['Merge Submission and Descendant Values']['Values JSON'])\nresult = {}\n\n# Loop over each value and remove nil's and join arrays\nvalues.each do |k, v|\n  # If the field is for a complex type (IE list)\n  if v.is_a?(Array)\n    unless v.empty?\n      # Check to see if it's a Hash (Attachments)\n      if v[0].is_a?(Hash)\n        output = ''\n        v.each_with_index do |val, index|\n          next unless val.key?('contentType')\n          output << if index == v.size - 1\n                      \"Attachment Name: #{val['name']} (#{val['contentType']})\"\n                    else\n                      \"Attachment Name: #{val['name']} (#{val['contentType']})\\n\"\n                    end\n        end\n        result[k] = output\n      else\n        result[k] = v.join(', ')\n      end\n      end\n  # If the field is for a date/time\n  elsif v =~ /^\\d\\d\\d\\d-\\d\\d-\\d\\dT\\d\\d:\\d\\d:\\d\\d\\+00:00$/\n    result[k] = DateTime.parse(v).strftime('%c')\n  # If the field is for a date\n  elsif v =~ /^\\d\\d\\d\\d-\\d\\d-\\d\\d$/\n    result[k] = DateTime.parse(v).strftime('%D')\n  # If the field is for a time\n  elsif v =~ /^\\d\\d:\\d\\d$/\n    result[k] = DateTime.parse(v).strftime('%r')\n  # If the field is a normal field\n  else\n    result[k] = v\n  end\nend\n\ntable = ''\nresult.keys.each do |key|\n  table << \"#{key}: #{result[key]}\\n\"\nend\ntable\n\n  body = {\n    \"short_description\":values['Summary'],\n    \"description\":\"This Change Ticket was created from Kinetic Request Submission #{@submission['Id']}\\n\\nDETAILS:\\n#{table}\"\n  \n}\n\nbody.to_json\n %>",
          "required": true
        },
        {
          "dependsOnId": "",
          "dependsOnValue": "",
          "description": "",
          "id": "Summary",
          "label": "Summary",
          "menu": "",
          "value": "2875 Submission for <%=  @values['1 NAME'] %>  (<%=  @values['SYSTEM NAME'] %>)",
          "required": false
        },
        {
          "dependsOnId": "",
          "dependsOnValue": "",
          "description": "",
          "id": "Details",
          "label": "Details",
          "menu": "",
          "value": "<%= \nvalues = JSON.parse(@results['Merge Submission and Descendant Values']['Values JSON'])\nresult = {}\n\n# Loop over each value and remove nil's and join arrays\nvalues.each do |k, v|\n  # If the field is for a complex type (IE list)\n  if v.is_a?(Array)\n    unless v.empty?\n      # Check to see if it's a Hash (Attachments)\n      if v[0].is_a?(Hash)\n        output = ''\n        v.each_with_index do |val, index|\n          next unless val.key?('contentType')\n          output << if index == v.size - 1\n                      \"Attachment Name: #{val['name']} (#{val['contentType']})\"\n                    else\n                      \"Attachment Name: #{val['name']} (#{val['contentType']})\\n\"\n                    end\n        end\n        result[k] = output\n      else\n        result[k] = v.join(', ')\n      end\n      end\n  # If the field is for a date/time\n  elsif v =~ /^\\d\\d\\d\\d-\\d\\d-\\d\\dT\\d\\d:\\d\\d:\\d\\d\\+00:00$/\n    result[k] = DateTime.parse(v).strftime('%c')\n  # If the field is for a date\n  elsif v =~ /^\\d\\d\\d\\d-\\d\\d-\\d\\d$/\n    result[k] = DateTime.parse(v).strftime('%D')\n  # If the field is for a time\n  elsif v =~ /^\\d\\d:\\d\\d$/\n    result[k] = DateTime.parse(v).strftime('%r')\n  # If the field is a normal field\n  else\n    result[k] = v\n  end\nend\n\ntable = ''\nresult.keys.each do |key|\n  table << \"#{key}: #{result[key]}\\n\"\nend\ntable\n\n\"This Change Ticket was created from Kinetic Request Submission #{@submission['Id']}\\n\\nDETAILS:\\n#{table}\"%>",
          "required": false
